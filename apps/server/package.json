{
  "name": "@proj-airi/server",
  "type": "module",
  "version": "0.8.5-beta.3",
  "private": true,
  "scripts": {
    "apply:env": "dotenvx run -f .env -f .env.local --overload --ignore=MISSING_ENV_FILE",
    "auth:generate": "pnpm run apply:env -- better-auth generate --config src/scripts/auth.ts --output src/schemas/accounts.ts -y",
    "dev": "pnpm run apply:env -- tsx --watch src/app.ts",
    "start": "tsx src/app.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "pnpm run apply:env -- drizzle-kit push"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.52.0",
    "@electric-sql/pglite": "catalog:",
    "@guiiai/logg": "catalog:",
    "@hono/node-server": "^1.19.9",
    "@proj-airi/drizzle-orm-browser-migrator": "^0.1.4",
    "@proj-airi/server-schema": "workspace:*",
    "better-auth": "^1.4.19",
    "drizzle-orm": "^0.45.1",
    "drizzle-valibot": "catalog:",
    "hono": "catalog:",
    "injeca": "catalog:",
    "postgres": "^3.4.8",
    "stripe": "catalog:",
    "tsx": "^4.21.0",
    "valibot": "catalog:"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.4.19",
    "drizzle-kit": "^0.31.9"
  }
}
