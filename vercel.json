    "ANTHROPIC_MODEL": "",
    "GOOGLE_GENERATIVE_AI_API_KEY": "",
    "GOOGLE_GENERATIVE_AI_BASE_URL": "https://generativelanguage.googleapis.com/v1beta/openai/",
    "GOOGLE_GENERATIVE_AI_MODEL": "",
    "DEEPSEEK_API_KEY": "",
